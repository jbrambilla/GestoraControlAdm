@model EGestora.GestoraControlAdm.Application.ViewModels.ClienteViewModel

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal">
        <span aria-hidden="true">×</span><span class="sr-only">Close</span>
    </button>
    <h4 class="modal-title">@Model.PessoaJuridica.RazaoSocial</h4>
</div>

<table class="table">
<tr>
    <th>
        Descrição do Serviço
    </th>
    <th>
        Valor
    </th>
</tr>

@foreach (var item in Model.ClienteServicoList)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Servico.Descricao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Valor)
        </td>
    </tr>
}

</table>

<div class="modal-footer">
    <input type="button" class="btn btn-success" data-dismiss="modal" value="Ok" />
</div>
